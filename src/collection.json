{
  "extends": ["@schematics/angular"],
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "my-service": {
      "description": "Generate a service in the project",
      "factory": "./my-service/index#myService",
      "schema": "./my-service/schema.json"
    },
    "ngrx-store": {
      "description": "Generate a store in the project",
      "factory": "./ngrx-store/index#ngRxStore",
      "schema": "./ngrx-store/schema.json"
    }
  }
}
