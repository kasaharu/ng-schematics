    "test": "yarn build && jasmine src/**/*.spec.js",
    "prettier": "prettier --write './**/*.ts'",
    "prepublishOnly": "yarn build && npm pack",
    "publish": "npm publish --access=public ./kasaharu-ng-schematics*.tgz"
