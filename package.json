{
  "name": "@kasaharu/ng-schematics",
  "version": "0.0.2",
  "description": "Custom schematics",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "generate:adapter": "schematics .:adapter --debug false",
    "generate:store": "schematics .:store --debug false",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "prettier": "prettier --write './**/*.ts'"
  },
  "keywords": [
    "schematics"
  ],
  "author": "kasaharu",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "bin": {
    "ng-schematics": "./bin/ng-schematics.js"
  },
  "dependencies": {
    "@angular-devkit/core": "^8.3.12",
    "@angular-devkit/schematics": "^8.3.12",
    "@types/jasmine": "^3.3.9",
    "@types/node": "^8.0.31",
    "jasmine": "^3.3.1",
    "typescript": "~3.5.3"
  },
  "devDependencies": {
    "@angular-devkit/schematics-cli": "^0.803.12",
    "prettier": "1.18.2"
  }
}
