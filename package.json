  },
  "dependencies": {
    "@angular-devkit/core": "^9.1.1",
    "@angular-devkit/schematics": "^9.1.1",
    "@angular-devkit/schematics-cli": "^0.901.1",
    "@types/jasmine": "^3.3.9",
    "@types/node": "^8.0.31",
    "jasmine": "^3.3.1",
    "typescript": "~3.5.3"
  },
  "devDependencies": {
    "@schematics/angular": "^9.1.1",
    "prettier": "2.2.1"
  }
}
